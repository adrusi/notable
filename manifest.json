{
	"manifest_version": 2,
	"name": "Notable",
	"version": "1.1",
	"description": "Makes all tabs appear in new windows. Useful for tiling window managers.",
	"applications": {
		"gecko": {
			"id": "notable@adrusi.com",
			"strict_min_version": "57.0"
		}
	},
	"background": {
		"scripts": ["notable.js"]
	},
	"permissions": [
		"tabs"
	]
}

